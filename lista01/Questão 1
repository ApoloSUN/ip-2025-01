programa {

  funcao inicio() {

    real N1, N2, N3, media

    escreva("Digite trÃªs notas: ")
    leia (N1)
    leia (N2)
    leia (N3)

    media = (N1 + N2 + N3) / 3

    se (media >= 6) {
      escreva("MEDIA: ", media, "\n")
      escreva("APROVADO")
    
    } senao {
      escreva("MEDIA: ", media, "\n")
      escreva("REPROVADO")
    }
    
  }
}
